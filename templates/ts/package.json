      "lint:js": "eslint",
      "lint:js:fix": "npm run lint:js -- --fix",
      "lint:types": "npm run typecheck",
      "lint": "run-p lint:*"
    },
    "dependencies": {},
    "devDependencies": {
      "@commitlint/cli": "^19.8.1",
      "@commitlint/config-conventional": "^19.8.1",
      "@gravity-ui/eslint-config": "^4.1.0",
      "@gravity-ui/prettier-config": "^1.1.0",
      "@gravity-ui/tsconfig": "^1.0.0",
      "cross-env": "^7.0.3",
      "eslint": "^9.28.0",
      "globals": "^16.5.0",
      "husky": "^9.1.7",
      "nano-staged": "^0.8.0",
      "npm-run-all": "^4.1.5",
      "prettier": "^3.5.3",
      "typescript": "^5.9.3"
    },
    "nano-staged": {
      "*.{js,jsx,ts,tsx}": [
        "eslint --fix --quiet"
      ]
    }
  }